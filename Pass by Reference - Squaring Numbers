#include <iostream>
using namespace std;


void squareNumbers(int &a, int &b, int &c) {
    a = a * a;
    b = b * b;
    c = c * c;
}

int main() {
    int num1 = 2, num2 = 3, num3 = 4;
    cout << "Before function call: " << num1 << ", " << num2 << ", " << num3 << endl;
    
    squareNumbers(num1, num2, num3); 
    
    cout << "After function call: " << num1 << ", " << num2 << ", " << num3 << endl;
    
    return 0;
}
