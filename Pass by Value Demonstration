#include <iostream>
using namespace std;


void increment(int num) {
    num = num + 1;  
    cout << "Inside function: " << num << endl; 
}

int main() {
    int value = 10;
    cout << "Before function call: " << value << endl;
    
    increment(value); 
    
    cout << "After function call: " << value << endl; 
    
    return 0;
